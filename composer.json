{
  "name": "codexten/yii-metronic4",
  "type": "library",
  "description": "Yii Metronic theme integration",
  "require": {
    "codexten/yii-web": "~2.0",
    "codexten/yii-settings": "~2.0",
    "yii2mod/yii2-sweet-alert": "1.3",
    "codexten/yii-bootstrap3": "~2.0",
    "bower-asset/jquery": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "codexten\\yii\\metronic4\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "config-plugin": {
      "params": "src/config/params.php",
      "metronic4": [
        "$bootstrap3",
        "src/config/metronic4.php"
      ]
    }
  }
}
